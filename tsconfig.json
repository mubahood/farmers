{
  "name": "app3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-axios": "^3.2.4",
    "vue-router": "^3.5.2",
    "vuetify": "^2.5.6",
    "vuex": "^3.6.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true
    },
    "extends": ["plugin:vue/essential", "eslint:recommended"],
    "rules": {
      "no-unused-vars": "off",
      "eqeqeq": "off",
      "curly": "error",
      "quotes": ["error", "double"]
    },
    "parserOptions": {
      "ecmaVersion": 7,
      "parser": "babel-eslint"
    },
    "no-unused-vars": "off"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  }
}
